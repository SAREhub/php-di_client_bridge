{
  "name": "sarehub/client_di_bridge",
  "description": "PHP-DI Client bridge is a SAREhub PHP Client bridge for php-di library",
  "license": "Apache-2.0",
  "require-dev": {
    "phpunit/phpunit": "6.*",
    "mockery/mockery": "1.*"
  },
  "require": {
    "php": "^7.1",
    "sarehub/client": "0.7.*",
    "php-di/php-di": "^6.0"
  },
  "autoload": {
    "psr-4": {
      "SAREhub\\": "src/SAREhub"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SAREhub\\": "tests/SAREhub"
    }
  }
}
