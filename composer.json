{
  "name": "sarehub/php-di_client_bridge",
  "description": "PHP-DI Client bridge is a SAREhub PHP Client bridge for php-di library",
  "license": "Apache-2.0",
  "require-dev": {
    "phpunit/phpunit": "6.*",
    "mockery/mockery": "1.*"
  },
  "require": {
    "php": "^7.1",
    "monolog/monolog": "1.*",
    "respect/validation": "1.*",
    "php-amqplib/php-amqplib": "^2.7",
    "sarehub/commons": "0.7.*",
    "sarehub/client": "0.5.*",
    "pimple/pimple": "3.*",
    "php-di/php-di": "^6.0"
  },
  "autoload": {
    "psr-4": {
      "SAREhub\\": "src/SAREhub"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SAREhub\\": "tests/SAREhub"
    }
  }
}
